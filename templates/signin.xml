<fest:template xmlns:fest="http://fest.mail.ru" context_name="json">
	<div class = "informationBg_signin">
		<div class = "informationMessage_signin"> ds</div>
	</div>
	<div class = "allSignin">
		<div class="button__top">
				<p class="logo"> NotPvPTowerDefence </p>
		</div>
		<p class="hend">
				Authorization
		</p>
		<form method="POST" action="/api/v1/auth/signin" class="signin_form">
			<div class="cusInput">
				<label class="label label__login">
					Login
				</label>
				<input  class="input login-signin" type="text" name="login" value=""></input>
			</div>
			<div class="cusInput">
				<label class="label label__password">
					Password
				</label>
				<!--<input class="input" type="password" name="password" required="" value=""></input>-->
				<input  class="input password-signin" type="password" name="password" value=""></input>
			</div>
			<div class="cusInput">
				<input  class="input buts-signin" type="submit" value="Submit" disabled = "true"></input>
			</div>
			<div class="js-back backButton">
				<a href="#" class="textButton">&#9668; Back</a>
			</div>
		</form>
	</div>
</fest:template>